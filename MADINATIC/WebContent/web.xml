<?xml version="1.0" encoding="UTF-8"?>
<wep_app>
	<servlet>
		<servlet_name>Servlet</servlet_name>
		<servlet_class>com.madinatic.servlet.Servlet</servlet_class>
	</servlet>

	<servlet_mapping>
		<servlet_name>Servlet</servlet_name>
		<servlet_pattern>/Servlet</servlet_pattern>
	</servlet_mapping>
	
	<jsp-config>
		<jsp-property-group>
			<url-pattern>*.jsp</url-pattern>
			<include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
		</jsp-property-group>
	</jsp-config>
</wep_app>